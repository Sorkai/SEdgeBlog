{
    "name": "sedgeblog",
    "private": true,
    "version": "0.1.0",
    "packageManager": "pnpm@10.30.1",
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild",
            "sharp",
            "swup"
        ]
    },
    "scripts": {
        "dev": "pnpm --filter @sedge/web dev",
        "build": "pnpm run fetch:base && pnpm --filter @sedge/web build",
        "fetch:base": "node ./scripts/fetch-base-data.mjs"
    }
}